{
  "name": "@hyeokjaelee/stylelint-config",
  "version": "1.1.6",
  "author": "HyeokjaeLee <leehyeokjae97@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/HyeokjaeLee/code-convention-config",
  "main": "index.js",
  "files": [
    "index.js",
    "css-in-js.js"
  ],
  "keywords": [
    "stylelint",
    "css",
    "scss",
    "styleguide"
  ],
  "scripts": {
    "rule": "stylelint './**/*.{js,jsx,ts,tsx,css,scss,sass}'",
    "rule:fix": "stylelint './**/*.{js,jsx,ts,tsx,css,scss,sass}' --fix",
    "deploy": "npm publish --access=public",
    "version": "npm version"
  },
  "dependencies": {
    "stylelint": "^15.6.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3"
  },
  "peerDependencies": {
    "stylelint": "^14.0.0"
  },
  "optionalDependencies": {
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "prettier": "^2.8.8"
  }
}
