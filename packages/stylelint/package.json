{
  "name": "@hyeokjae/stylelint-config",
  "version": "1.0.0",
  "author": "HyeokjaeLee <leehyeokjae97@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/HyeokjaeLee/hyeokjae-convention-library",
  "main": "index.js",
  "files": [
    "rules",
    "index.js",
    "frontend.js",
    "prettierrc.js",
    "stylelint.js"
  ],
  "keywords": [
    "eslint",
    "eslintconfig",
    "prettier",
    "stylelint",
    "javascript",
    "typescript",
    "styleguide",
    "es2015",
    "es2016",
    "es2017",
    "es2018"
  ],
  "scripts": {
    "rule:eslint": "eslint './**/*.{js,jsx,ts,tsx}'",
    "rule:eslint:fix": "npm run lint -- --fix",
    "rule:stylelint": "stylelint './**/*.{js,jsx,ts,tsx}'",
    "rule:stylelint:fix": "stylelint './**/*.{js,jsx,ts,tsx}' --fix",
    "rule:prettier": "prettier '**/*.{json,yaml,md,js,jsx,ts,tsx}' --check",
    "rule:prettier:fix": "prettier '**/*.{json,yaml,md,js,jsx,ts,tsx}' --write",
    "rule:all": "npm run code-convetion:eslint && npm run code-convetion:stylelint && npm run code-convetion:prettier",
    "rule:all:fix": "npm run code-convetion:eslint:fix && npm run code-convetion:stylelint:fix && npm run code-convetion:prettier:fix",
    "deploy": "npm publish",
    "version": "npm version"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "next": "^13.3.2",
    "postcss": "^8.4.23",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "stylelint": "^15.6.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^2.0.0",
    "stylelint": "^14.0.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@3.5.0"
}
